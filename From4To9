using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Numbers
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = 4;
            int b = 9;

            for (int i = a; i <= b; i++)
            {
                for (int j = 0; j < i; j++)
                {
                    Console.Write(i.ToString() + " ");
                }
               Console.WriteLine();
            }
            Console.ReadKey();
        }
    }
}
